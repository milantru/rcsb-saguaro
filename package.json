{
  "name": "@rcsb/rcsb-saguaro",
  "version": "3.0.0-dev-js-subjects.3",
  "description": "RCSB 1D Feature Viewer",
  "main": "build/RcsbFv.js",
  "files": [
    "lib/",
    "build/rcsb-saguaro.js"
  ],
  "scripts": {
    "devServer": "webpack-dev-server --config webpack.server.dev.config.js",
    "build": "webpack --config ./webpack.config.js",
    "buildApp": "npm run build-tsc && npm run build-commonjs && npm run build && npm run cpStyles",
    "buildAll": "npm run buildApp && npm run buildAllDoc",
    "buildDoc": "typedoc --excludeExternals --externalPattern \"**/node_modules/**\" && sed -i '' '/---/d' docs/index.html",
    "cpExamples": "ncp examples docs/examples",
    "cpStyles": "ncp src/scss lib/scss && ncp src/scss lib/commonjs/scss",
    "buildAllDoc": "npm run buildDoc && npm run cpExamples",
    "packApp": "npm run buildApp && npm pack",
    "publishApp": "npm publish --tag dev-js-subjects",
    "build-tsc": "tsc",
    "build-commonjs": "tsc --build tsconfig.commonjs.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rcsb/rcsb-saguaro.git"
  },
  "keywords": [
    "1D",
    "sequence",
    "annotations",
    "gene",
    "protein"
  ],
  "author": "Joan Segura Mora <joan.segura@rcsb.org>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.2",
    "@floating-ui/dom": "^1.5.3",
    "@types/d3": "^7.4.2",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/react-transition-group": "^4.4.8",
    "@types/uniqid": "^5.3.3",
    "babel-loader": "^9.1.3",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "ncp": "^2.0.0",
    "path-browserify": "^1.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.0.2",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.5.0",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.3",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typescript": "^5.2.2",
    "uniqid": "^5.4.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@d3fc/d3fc-sample": "^5.0.1",
    "array-move": "^4.0.0",
    "d3": "^7.6.1",
    "d3-shape": "^3.1.0",
    "react-transition-group": "^4.4.5",
    "rxjs": "^7.5.6"
  },
  "peerDependencies": {
    "boxicons": "^2.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "bugs": {
    "url": "https://github.com/rcsb/rcsb-saguaro/issues"
  },
  "homepage": "https://rcsb.github.io/rcsb-saguaro",
  "directories": {
    "doc": "docs",
    "example": "examples"
  }
}
